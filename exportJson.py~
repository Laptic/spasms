import json;

def createJsonFile(cursor, topic):
	data = {};
	data["tweets"] = [];
	data["tweets"].append({
		"created_at": "2019-11-20 03:00:00",
		"id": 123456789,
		"id_str": "123456789",
		"text": "My name is Bob",
		"user": {
			"id": 1,
			"name": "Bob"
		}
	});
	data["tweets"].append({
		"created_at": "2019-11-20 03:30:00",
		"id": 111111111,
		"id_str": "111111111",
		"text": "My name is Jane",
		"user": {
			"id":2,
			"name":"Jane"
		}
	});
	outputFile = open("output.json","w");
	json.dump(data["tweets"],outputFile);

createJsonFile(1,2);
